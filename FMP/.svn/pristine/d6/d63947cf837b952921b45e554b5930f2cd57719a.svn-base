<Window x:Class="Full_Metal_Planete.src.game.game_window.FullscreenWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Full_Metal_Planete.src.game.game_window"
        mc:Ignorable="d"
        Title="Full Metal Planete"
        WindowStartupLocation="CenterScreen"
        ResizeMode="NoResize"
        WindowStyle="{Binding FSWindowStyle}" 
        d:DesignHeight="1080" d:DesignWidth="1920"
        Height="Auto" Width="Auto"
        Background="Black"
        AllowDrop="True">
    <Grid>
        <ScrollViewer x:Name="_scrollPanel">
            <Canvas x:Name="_mapCanvas" Height="1750"/>
        </ScrollViewer>
        <Canvas x:Name="_uiCanvas"/>
    </Grid>
</Window>

<Window x:Class="Full_Metal_Planete.src.game.menus.RulesWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Full_Metal_Planete.src.game.menus"
        mc:Ignorable="d"
        Title="RulesWindow" 
        Height="600" 
        Width="800">
    <Grid>
        <Grid x:Name="gridTreeView" Margin="0,0,508,0">
            <TreeView x:Name="treeView" BorderThickness="1">
                <TreeViewItem Header="Jeu">
                    <Label x:Name="intro" 
                           Content="Introduction"
                           MouseLeftButtonDown="Intro_MouseLeftButtonDown"/>
                    <Label x:Name="units" 
                           Content="Units"
                           MouseLeftButtonDown="Units_MouseLeftButtonDown"/>
                    <Label x:Name="rules"
                           Content="Rules"
                           MouseLeftButtonDown="Rules_MouseLeftButtonDown"/>
                </TreeViewItem>
                <TreeViewItem Header="IAs">
                    <Label x:Name="AILevel"
                           Content="Change difficulty"
                           MouseLeftButtonDown="AILevel_MouseLeftButtonDown"/>
                    <Label x:Name="AIBehavior"
                           Content="Behavior"
                           MouseLeftButtonDown="AIBehavior_MouseLeftButtonDown"/>
                </TreeViewItem>
            </TreeView>
        </Grid>
        <Grid x:Name="gridTextView" Margin="289,0,0,0"/>
    </Grid>
</Window>

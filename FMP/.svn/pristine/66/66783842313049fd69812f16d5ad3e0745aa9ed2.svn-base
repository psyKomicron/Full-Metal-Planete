<Window x:Class="Full_Metal_Planete.src.game.menus.PreferencesWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Full_Metal_Planete.src.game.menus"
        mc:Ignorable="d"
        Title="Preferences" 
        Height="450" 
        Width="800"
        WindowStartupLocation="CenterScreen">
    <Grid>
        <Canvas x:Name="rightCanvas"
                Margin="209,0,0,0" 
                Height="419"
                Width="583"
                VerticalAlignment="Top" 
                HorizontalAlignment="Left"/>

        <Grid Margin="0,0,588,0">
            <StackPanel>
                <TreeView Background="Transparent" BorderThickness="0">
                    <TreeViewItem Header="Video">
                        <Label x:Name="videoSettings" 
                               Content="Video settings" 
                               MouseLeftButtonDown="VideoSettings_MouseLeftButtonDown"/>
                    </TreeViewItem>
                </TreeView>
            </StackPanel>
        </Grid>
    </Grid>
</Window>

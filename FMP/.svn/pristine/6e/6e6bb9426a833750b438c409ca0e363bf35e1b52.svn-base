using System;
using System.Collections.Generic;
using System.Text;
using Metier.Carte.Cases;
using Metier.Unites;

namespace Métier.Unites
{
    public abstract class Entité : IEntité
    {
        private Case position;

        public abstract TypeEntité Type();

        public Entité(Case position)
        {
            this.Position = position;
        }

        public Case Position
        {
            get => position;
            set => position = value;
        }

    }
}
